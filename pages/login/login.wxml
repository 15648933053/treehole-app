<view>
	<view class="tel">
		<van-cell-group>
			<van-field label="用户名：" type="number" placeholder="请输入用户名/手机号" border="{{ false }}" bind:input="setTel" />
		</van-cell-group>
	</view>

	<view class="password">
		<van-cell-group>
			<van-field label="密码：" type="password" placeholder="请输入密码" border="{{ false }}" bind:input="setPassword" />
		</van-cell-group>
	</view>

	<view class="btn" bindtap="login">
		<button class="btn1">登录</button>
	</view>
</view>

<view class="top">
	<navigator url="../register/register">
		<text>还没有注册？点我注册</text>
	</navigator>
</view>

<van-notify id="van-notify" />


